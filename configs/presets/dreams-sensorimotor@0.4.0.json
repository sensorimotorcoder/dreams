  "exceptions": {
    "proper_name_exceptions": ["godfrey", "goddard"],
    "idiom_exclusions.supernatural": ["oh my god"],
    "objects_require_determiner": ["ring", "mirror"],
    "negations": ["no", "not", "never"]
  },
  "notes": [
    "Extend lexicons through the API before committing changes to config/categories.yml."
